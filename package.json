{
  "name": "student-faculty-management-system",
  "version": "1.0.0",
  "description": "A comprehensive student and faculty management system",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd client && npm start",
    "dev:backend": "php artisan serve --port 8000",
    "build": "cd client && npm run build",
    "build:frontend": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "phpunit"
  },
  "keywords": ["student", "faculty", "management", "education"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
